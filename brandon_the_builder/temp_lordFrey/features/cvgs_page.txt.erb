DISCOUNTS:
  ALL_DISCOUNTS: M-P;ASS;M-L<%#= $effective_date > Date.new(2015,10,30) ? ';PD;CF;GC' : ''%> # after prec - 10/31/2015
  APPLICABLE_DISCOUNTS: <%#= applicable_discounts %>
  PROTECTIVE_DEVICE_DISCOUNT: PD

TEXTS:
  - MONTHLY_PREMIUM
  - ANNUAL_PREMIUM
  - PROPERTY_PROTECTION
  - 15K_MIN_100K_MAX
  - 20K_BRAND_NEW_BEL
  - PERSONAL_PROTECTION
  - PERSONAL_ITEMS_PROTECTION
  - DEDUCTIBLES
  - 10%_LIMIT_FROM_PERS_PROP
  - ADDITIONAL_PROTECTION
  - OTHER_COVERAGE_LIMITS
  - QUOTE_DISCLAIMER
  - # Headers:
  - STEPS
  - QUOTE_ID
  - SECURE
  - #Footers
  - COPYRIGHT

COVERAGES:
  - PERSONAL_PROPERTY:
      ELEMENT: personal_property_coverage_textbox_element
#      DEFAULT: <%= personal_property_amount %> #todo: really want to implement this properly?
      HELP: PERSONAL_PROPERTY_HELP
  - BRAND_NEW_BELONGINGS:
      <%= brand_new_based_on_pp %>
  - PERSONAL_LIABILITY:
      LIST: 100K-500K
      DEFAULT: '100K'
      HELP: PERSONAL_LIABILITY_HELP
  - MEDICAL_PAYMENTS:
      LIST: 1K-5K
      DEFAULT: '1K'
      HELP: MEDICAL_PAYMENTS_HELP
  - PERSONAL_INJURY_LIABILITY:
      LIST: {PERSONAL_INJURY_LIABILITY_COVERAGE_LIST: ACC_DEC}
      DEFAULT: 'DEC'
      HELP: PERSONAL_INJURY_LIABILITY_HELP
  - JEWELRY_WATCHES_FURS:
      LIST: 500-2.5K-5K
      DEFAULT: '.5K'
      HELP: JEWELRY_WATCHES_FURS_HELP
  - CAMERAS:
      LIST: 500-5K
      DEFAULT: '.5K'
      HELP: CAMERAS_HELP
  - COMPUTERS:
      LIST: 3K-10K
      DEFAULT: '3K'
      HELP: COMPUTERS_HELP
  - CREDIT_CARD:
      LIST: 1K-10K
      DEFAULT: '1K'
      HELP: CREDIT_CARD_HELP
  - MONEY:
      LIST: 100-500
      DEFAULT: '.1K'
      HELP: MONEY_HELP
  - SECURITIES:
      LIST: 500-1K
      DEFAULT: '.5K'
      HELP: SECURITIES_HELP
  - TOOLS:
      LIST: 500-5K
      DEFAULT: '.5K'
      HELP: TOOLS_HELP
  - FIREARMS:
      LIST: 500-5K
      DEFAULT: '.5K'
      HELP: FIREARMS_HELP
  - SILVERWARE:
      LIST: 1K-10K
      DEFAULT: '1K'
      HELP: SILVERWARE_HELP
  - ALL_PERILS:
      LIST: <%= all_peril_based_on_auto %>
      DEFAULT: <%= ap_default_based_on_auto %>
      HELP: ALL_PERILS_HELP
  - EARTHQUAKE_&_VOLCANIC_ERUPTION:
      LIST: 10-25%_DED
      DEFAULT: DEC
      HELP: EARTHQUAKE_&_VOLCANIC_ERUPTION_HELP

  - IDENTITY_THEFT_OR_FRAUD_EXPENSES:
      LIST: ACC_DEC
      DEFAULT: DEC
      HELP: IDENTITY_THEFT_OR_FRAUD_EXPENSES_HELP
  - THEFT_EXTENSION:
      LIST: ACC_DEC
      DEFAULT: DEC
      HELP: THEFT_EXTENSION_HELP
  - FIRE_DEPARTMENT_SERVICE_CHARGE:
      LIST: 500_750_1K
      HELP: FIRE_DEPARTMENT_SERVICE_CHARGE_HELP
  - WATER_BACKUP:
#     LABEL: WATER_BACKUP_SEWER_DRAIN
      LABEL: WATER_BACKUP
      LIST: <%= wb_based_on_pp %>
      DEFAULT: 'DEC'
      HELP: WATER_BACKUP_HELP
  - HOME_CARE_LIABILITY:
      <%= home_care_elements %>
  - BUILDING_ADDITIONS_ALTERATIONS:
      TEXT: <%= percent_of_pp 10 %>
      HELP: BUILDING_ADDITIONS_ALTERATIONS_HELP
  - LOSS_OF_USE:
      TEXT: <%= percent_of_pp 20 %>
      HELP: LOSS_OF_USE_HELP

LINKS:
  - <%= common_footer_links %>